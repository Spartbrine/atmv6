DROP TRIGGER update_typeCard  

</sql><sql name="Edit Trigger update_typeCard">DROP TRIGGER &quot;main&quot;.&quot;update_typeCard&quot;;
CREATE TRIGGER update_typeCard
AFTER INSERT ON cards
BEGIN
    UPDATE cards
    SET typeCard = 
        CASE 
            WHEN substr(card, 1, 1) = '4' THEN 'VISA'
            WHEN substr(card, 1, 1) = '5' THEN 'MASTERCARD'
            ELSE 'Otro'
        END;
END</sql><current_tab id="1"/></tab_sql></sqlb_project>
